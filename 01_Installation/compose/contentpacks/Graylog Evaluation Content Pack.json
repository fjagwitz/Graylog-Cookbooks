    {
      "v": "1",
      "type": {
        "name": "pipeline",
        "version": "1"
      },
      "id": "4a322031-d90e-4f59-882b-6615a62372e7",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Pipeline"
        },
        "description": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Pipeline"
        },
        "source": {
          "@type": "string",
          "@value": "pipeline \"Evaluation - Self-Monitoring Pipeline\"\nstage 0 match either\nrule \"Evaluation - Self-Monitoring Parser Graylog 1\"\nrule \"Evaluation - Self-Monitoring Parser MongoDB 1\"\nrule \"Evaluation - Self-Monitoring Parser Opensearch 1\"\nrule \"Evaluation - Self-Monitoring Parser Graylog Installer\"\nrule \"Evaluation - Self-Monitoring Parser Falco 1\"\nend"
        },
        "connected_streams": [
          {
            "@type": "string",
            "@value": "2916bb3d-7416-46b3-855a-ef36387d3abb"
          }
        ]
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "0dee04a7-8d53-410e-9c50-41562d6843ac",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Parser Opensearch 1"
        },
        "description": {
          "@type": "string",
          "@value": ""
        },
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Parser Opensearch 1\"\n\nwhen\n    contains(to_string($message.image_name),\"opensearch\")\n    \nthen\n    let mylog = grok(\n        value: to_string($message.message),\n        pattern: \"\\\\[%{TIMESTAMP_ISO8601:event_created}\\\\]%{SPACE}\\\\[%{WORD:event_severity}%{SPACE}\\\\]%{SPACE}\\\\[%{SPACE}%{GREEDYDATA:application_component}%{SPACE}\\\\]%{SPACE}\\\\[%{SPACE}%{HOSTNAME:event_source}%{SPACE}\\\\]%{GREEDYDATA:message}\"\n    );\n    \n    set_fields(mylog);\n    \n    rename_field(\n        old_field: \"created\",\n        new_field: \"container_created\"\n    );\n    \n    remove_multiple_fields(\n        pattern: \"[A-Z0-9_-]+\",\n        names: [\"container_id\",\"image_id\",\"tag\"]\n    );\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "5481c459-47e0-4b97-9233-00229316206d",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Parser Graylog 1"
        },
        "description": null,
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Parser Graylog 1\"\n\nwhen\n    contains(to_string($message.image_name),\"graylog\")\n    //true\n    \nthen\n    let mylog = grok(\n        value: to_string($message.message),\n        pattern: \"%{TIMESTAMP_ISO8601:event_created}%{SPACE}%{WORD:event_severity}%{SPACE}:%{SPACE}%{NOTSPACE:application_component}%{SPACE}-%{SPACE}%{GREEDYDATA:message}\"\n    );\n    \n    set_fields(mylog);\n    \n    rename_field(\n        old_field: \"created\",\n        new_field: \"container_created\"\n    );\n    \n    remove_multiple_fields(\n        pattern: \"[A-Z0-9_-]+\",\n        names: [\"container_id\",\"image_id\",\"tag\"]\n    );\n    \nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "a8f9b5d3-22ec-467f-9304-979e50fe7923",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Parser Graylog Installer"
        },
        "description": null,
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Parser Graylog Installer\"\n\nwhen\n    contains(to_string($message.message),\"GRAYLOG-\") \n    //true\n    \nthen\n    let mylog = grok(\n        value: to_string($message.message),\n        pattern: \"%{TIMESTAMP_ISO8601}%{SPACE}%{HOSTNAME:event_source}%{SPACE}%{DATA:application_component}:%{SPACE}\\\\[%{WORD:event_severity}\\\\]%{SPACE}-%{SPACE}%{GREEDYDATA:message}\"\n    );\n    \n    set_fields(mylog);\n    remove_multiple_fields(\n        pattern: \"[A-Z0-9_-]+\"\n    );\n    //set_field(\"touched\",true);\n\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "76be8c7c-1077-4163-815a-6f78ec93dda9",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Parser MongoDB 1"
        },
        "description": null,
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Parser MongoDB 1\"\n\nwhen\n    contains(to_string($message.image_name),\"mongo\")\n    //true\nthen\n    \n    let mylog = flatten_json(to_string(parse_json(replace(to_string($message.message),\"$\"))),\"flatten\",true);\n    \n    set_fields(to_map(mylog));\n\n    rename_field(\"attr_message_msg\",\"message\");\n    rename_field(\"attr_message_verbose_level\",\"event_severity\");\n    rename_field(\"attr_message_session_name\",\"application_component\");\n    rename_field(\"t_date\",\"event_created\");\n    rename_field(\"created\",\"container_created\");\n    \n    remove_multiple_fields(\n        pattern: \"attr_[A-Za-z-_]*\",\n        names: [\"c\",\"container_id\",\"ctx\",\"id\",\"image_id\",\"msg\",\"s\",\"tag\"]\n    );\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "b431c79e-1ae8-4a99-89ba-f412fc6dec2a",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Cleanup"
        },
        "description": null,
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Cleanup\"\n\nwhen\n    NOT has_field(\"event_severity\")\n    \nthen\n    drop_message();\n\nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
    {
      "v": "1",
      "type": {
        "name": "pipeline_rule",
        "version": "1"
      },
      "id": "bf462c5c-b3ac-4352-a54d-80081a90420e",
      "data": {
        "title": {
          "@type": "string",
          "@value": "Evaluation - Self-Monitoring Parser Falco 1"
        },
        "description": null,
        "source": {
          "@type": "string",
          "@value": "rule\"Evaluation - Self-Monitoring Parser Falco 1\"\n\nwhen\n    contains(to_string($message.image_name),\"falco\")\n    //true\n    \nthen\n    let mylog = grok(\n        value: to_string($message.message),\n        pattern: \"%{TIMESTAMP_ISO8601:event_created}:%{SPACE}%{GREEDYDATA:short_message}%{SPACE}\\\\|\"\n    );\n    \n    let mykeys = key_value(to_string($message.message));\n    \n    set_fields(mylog);\n    set_fields(mykeys);\n    set_field(\"event_severity\",\"WARN\");\n    \n    rename_field(\n        old_field: \"created\",\n        new_field: \"container_created\"\n    );\n    \n    remove_multiple_fields(\n        pattern: \"[A-Z0-9_-]+\",\n        names: [\"container_id\",\"image_id\",\"tag\"]\n    );\n    \nend"
        }
      },
      "constraints": [
        {
          "type": "server-version",
          "version": ">=6.3.3+700dd8f"
        }
      ]
    },
